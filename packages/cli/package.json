{
  "name": "@exomoon/erc721-layered-assets-cli",
  "version": "1.0.0",
  "description": "Exomoon CLI package for ERC721 Layered assets.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "bin": "./dist/esm/index.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "test-verbose": "jest --verbose --maxWorkers=1 --passWithNoTests",
    "test": "yarn test-verbose --silent",
    "coverage": "yarn test --coverage",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "clean-build": "yarn clean && yarn build",
    "clean": "rm -rf dist && rm -rf dist",
    "lint": "eslint . --ext .js,.ts",
    "docs": "typedoc --plugin typedoc-plugin-markdown --out docs src/index.ts",
    "start": "yarn build && node --experimental-specifier-resolution=node dist/esm/index.js"
  },
  "dependencies": {
    "canvas": "2.11.2",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.17",
    "@exomoon/erc721-layered-assets-client": "1.0.3"
  },
  "devDependencies": {
    "@burneeble/eslint-plugin-burneeble": "^1.0.12",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.12",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.1.6"
  },
  "files": [
    "/dist"
  ]
}